{
  "id": "arangodb-framework",
  "args": [
    "framework",
    "--master", "{{mesos/master}}",
    "--zk", "{{arangodb/zk}}"
  ],
  "cpus": 0.2,
  "mem": 64.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "arangodb/arangodb-mesos",
      "network": "HOST"
    }
  }
}
